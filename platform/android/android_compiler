ANDROID_SIZE = $(ANDROID_CROSS_COMPILE)arm-linux-androideabi-size
ANDROID_STRIP = $(ANDROID_CROSS_COMPILE)arm-linux-androideabi-strip
ANDROID_AR = $(ANDROID_CROSS_COMPILE)arm-linux-androideabi-ar
