tick : std.Trigger(data="This is some POST data", tick=2.0)
cmd : net.HTTPPost()
s_out : io.Print()
b_out : io.Print()
h_out : io.Print()


tick.data > /"http://httpbin.org/post"/ cmd.URL
tick.data > cmd.data
{"content-type": "application/json"} > cmd.headers
null > cmd.auth
null > cmd.params
cmd.status > s_out.token
cmd.data > b_out.token
cmd.headers > h_out.token
